<!DOCTYPE html>
<html>
<head>
    {% block stylesheet %}
        <link href="{{ asset('bundles/balletwaytoc/assets/css/main.min.css') }}" rel="stylesheet" type="text/css" />
        <link href='https://fonts.googleapis.com/css?family=Yesteryear|Mr+Dafoe|Oswald|Petit+Formal+Script' rel='stylesheet' type='text/css'>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>waytoc.com - street style</title>
        <link rel="apple-touch-icon" sizes="57x57" href="/apple-icon-57x57.png">
        <link rel="apple-touch-icon" sizes="60x60" href="/apple-icon-60x60.png">
        <link rel="apple-touch-icon" sizes="72x72" href="/apple-icon-72x72.png">
        <link rel="apple-touch-icon" sizes="76x76" href="/apple-icon-76x76.png">
        <link rel="apple-touch-icon" sizes="114x114" href="/apple-icon-114x114.png">
        <link rel="apple-touch-icon" sizes="120x120" href="/apple-icon-120x120.png">
        <link rel="apple-touch-icon" sizes="144x144" href="/apple-icon-144x144.png">
        <link rel="apple-touch-icon" sizes="152x152" href="/apple-icon-152x152.png">
        <link rel="apple-touch-icon" sizes="180x180" href="/apple-icon-180x180.png">
        <link rel="icon" type="image/png" sizes="192x192"  href="/android-icon-192x192.png">
        <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="96x96" href="/favicon-96x96.png">
        <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png">
        <link rel="manifest" href="/manifest.json">
        <meta name="msapplication-TileColor" content="#ffffff">
        <meta name="msapplication-TileImage" content="/ms-icon-144x144.png">
        <meta name="theme-color" content="#ffffff">
    {%  endblock %}
</head>
<body>

{% include 'BalletWaytocBundle:Layout:header.html.twig' %}

<div class="container-wrapper">
    <div class="container">
        <div class="row">
            {% block content %}

            {% endblock content %}

            <div class="col-sm-3 nopadding clearfix">
                {% block sidebar %}
                    {% include 'BalletWaytocBundle:Layout:sidebar.html.twig' %}
                {% endblock %}

            </div>
        </div>
    </div>
</div>
{% block footer %}
    <footer class="footer-wrap">
        <section class="container pt30">
            <ul class="list-unstyled list-inline social-icons col-lg-6">
                <li>
                    <a href="https://www.facebook.com/KaunasStreetStyle" target="_blank">
                        <i class="fa fa-facebook"></i>
                    </a>
                </li>
                <li>
                    <a href="https://www.pinterest.com/source/waytoc.com/" target="_blank">
                        <i class="fa fa-pinterest"></i>
                    </a>
                </li>
                <li>
                    <a href="https://www.instragram.com" target="_blank">
                        <i class="fa fa-instagram"></i>
                    </a>
                </li>
                <li>
                    <a href="https://www.google.com" target="_blank">
                        <i class="fa fa-google-plus"></i>
                    </a>
                </li>
            </ul>
            <ul class="list-inline col-lg-6 mt10 text-right">
                <li class="white-smoke">
                    <i class="fa fa-copyright white-smoke"></i> {{"now"|date('Y') }} Waytoc.com. All rights reserved.
                </li>
            </ul>
        </section>
    </footer>
{% endblock %}
{% block javascript %}
    <script src="{{ asset('bundles/balletwaytoc/assets/js/main.min.js') }}"></script>
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-45622151-1', 'waytoc.com');
        ga('send', 'pageview');

    </script>
{% endblock %}
</body>
</html>